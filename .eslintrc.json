{
    "extends": [
        "standard",
        "plugin:react/recommended",
        "plugin:tailwindcss/recommended",
        "prettier"
    ],
    "rule": {
        "max-len": [2,250],
        "no-muliple-empty-lines": [
            "error",
            {
                "max": 1,
                "maxEOF": 1
            }
        ],
        "object-curly-newline": 0
    }
}